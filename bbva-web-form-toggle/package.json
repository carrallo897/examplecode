{
  "name": "@bbva-web-components/bbva-web-form-toggle",
  "version": "1.0.4",
  "description": "Toggles will toggle the state of a page or object.",
  "keywords": [
    "form",
    "toggle",
    "web"
  ],
  "main": "index.js",
  "files": [
    "/src/",
    "/locales/",
    "*.js",
    "!**/*.scss"
  ],
  "scripts": {
    "test": "npx karma start ../../_karma/conf.js",
    "test:compatibility": "npm run test -- --force-polyfills --compatibility all",
    "test:compatibility:watch": "npm run test:compatibility --  --auto-watch=true --single-run=false",
    "test:prune-snapshots": "npm run test -- --prune-snapshots",
    "test:update-snapshots": "npm run test -- --update-snapshots",
    "test:watch": "npm run test -- --auto-watch=true --single-run=false"
  },
  "dependencies": {
    "@bbva-web-components/bbva-core-lit-helpers": "^1.0.4",
    "@bbva-web-components/bbva-core-switch": "^1.0.0",
    "@bbva-web-components/bbva-foundations-styles": "^2.3.5",
    "lit-element": "^2.0.0",
    "lit-html": "^1.0.0"
  },
  "devDependencies": {
    "@bbva-web-components/bbva-web-divider": "^1.0.4",
    "@cells/cells-component-core": "^3.0.0"
  }
}
